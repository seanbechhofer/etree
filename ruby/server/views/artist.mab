div.container do
  div.row do
    div.send(:'col-md-12') do
      h1 name
      div do
        p do
          a query, :href => query
        end
        if results.size > 0 then
          ol do |list|	  
            results.each do |result|
              list.li {
                a result[:eventName], :href => "/event/" + URIEncode(result[:event])
              }
            end
          end
        else
          p "No Events"
        end
      end
    end
  end
end
